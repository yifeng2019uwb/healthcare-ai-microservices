# Neon Database Connection Environment Variables
# Copy this file to neon-connection.env and fill in your actual values

# Database Connection
NEON_HOST=your-neon-host.neon.tech
NEON_PORT=5432
NEON_DATABASE=medconnect-healthcase
NEON_USERNAME=your-username
NEON_PASSWORD=your-password

# Connection String (alternative)
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Optional: Connection Pool Settings
NEON_MAX_CONNECTIONS=10
NEON_MIN_CONNECTIONS=2
NEON_CONNECTION_TIMEOUT=30
