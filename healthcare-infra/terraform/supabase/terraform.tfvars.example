# Supabase Database Configuration Template
# Healthcare AI Microservices - Production Configuration
#
# Copy this file to terraform.tfvars and fill in your actual values
# terraform.tfvars is gitignored and should NOT be committed

# Supabase Database Connection Details
# Get these values from your Supabase project dashboard
supabase_host = "db.your-project-ref.supabase.co"
supabase_port = 5432
supabase_database = "postgres"
supabase_username = "postgres"
supabase_password = "your-supabase-password"

